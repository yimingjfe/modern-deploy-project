{
  "name": "modern-deploy-project",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev",
    "build": "npx modern deploy",
    "deploy": "modern deploy",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "lint": "modern lint",
    "prepare": "husky install",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": "=18.20.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@modern-js/plugin-bff": "0.0.0-next-20240425064656",
    "@modern-js/plugin-express": "2.49.3",
    "@modern-js/runtime": "0.0.0-next-20240425064656",
    "express": "^4.19.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "0.0.0-next-20240425064656",
    "@modern-js/app-tools": "2.49.3-alpha.18",
    "@modern-js/eslint-config": "0.0.0-next-20240425064656",
    "@modern-js/tsconfig": "0.0.0-next-20240425064656",
    "@types/express": "^4.17.21",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.7",
    "@types/react": "~18.0.26",
    "@types/react-dom": "~18.0.10",
    "husky": "~8.0.1",
    "lint-staged": "~13.1.0",
    "prettier": "~2.8.1",
    "rimraf": "~3.0.2",
    "typescript": "~5.0.4"
  }
}
